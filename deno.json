{
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.ns",
      "dom"
    ],
    "jsx": "preserve",
    "jsxImportSource": "npm:solid-js:",
    "jsxFactory": "Solid.createElement"
  },
  "lock": false,
  "tasks": {
    "build": "deno run -A './scripts/build.ts'",
    "serve": "deno run -A './.out/server.js'",
    "dev": "deno run -A './scripts/dev.ts'"
  },
  "imports": {
    "solid-js": "https://esm.sh/solid-js@1.7.5?",
    "solid-js/web": "https://esm.sh/solid-js@1.7.5/web?",
    "esbuild": "https://deno.land/x/esbuild@v0.17.15/mod.js",
    "esbuild-plugin-solid": "https://esm.sh/esbuild-plugin-solid@0.4.2"
  },
  "client_overrides": {
    "solid-js": "https://esm.sh/solid-js?target=esnext",
    "solid-js/web": "https://esm.sh/solid-js/web?target=esnext"
  }
}
